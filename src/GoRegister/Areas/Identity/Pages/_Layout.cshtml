<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Go Register</title>
    <!-- Custom fonts for this template-->
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Custom styles for this template-->
    <environment names="Development">
        <link rel="stylesheet" href="/assets/vendor/font-awesome-5/css/all.css" asp-append-version="true" />
        <link rel="stylesheet" href="/assets/admin/css/admin.css" asp-append-version="true">
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="/assets/vendor/font-awesome-5/css/all.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="/assets/admin/css/admin.min.css" asp-append-version="true">
    </environment>
</head>
<body class="bg-gradient-primary">
    <div class="container">
        <!-- Outer Row -->
        <div class="row justify-content-center">
            <div class="col-xl-10 col-lg-12 col-md-9">
                <div class="card o-hidden border-0 shadow-lg my-5">
                    <div class="card-body">
                        <!-- Nested Row within Card Body -->
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    <environment names="Development">
        <script src="/assets/vendor/jquery/dist/jquery.js" asp-append-version="true"></script>
        <script src="/assets/vendor/bootstrap/dist/js/bootstrap.js" asp-append-version="true"></script>
        <script src="/assets/vendor/jquery-easing/jquery.easing.js" asp-append-version="true"></script>
        <script src="/assets/js/vendor/sbadmin-2/sb-admin-2.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="/assets/vendor/jquery/dist/jquery.min.js" asp-append-version="true"></script>
        <script src="/assets/vendor/bootstrap/dist/js/bootstrap.min.js" asp-append-version="true"></script>
        <script src="/assets/vendor/jquery-easing/jquery.easing.min.js" asp-append-version="true"></script>
        <script src="/assets/js/vendor/sbadmin-2/sb-admin-2.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("scripts", required: false)
</body>
</html>
