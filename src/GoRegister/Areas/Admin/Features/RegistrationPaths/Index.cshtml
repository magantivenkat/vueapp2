@model GoRegister.Areas.Admin.Features.RegistrationPaths.Index.Result
@{
    ViewData["Title"] = "Registration Paths";
}

<h1>RegistrationPaths</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td class="text-right">
                    <a-proj asp-action="Edit" asp-route-id="@item.Id">Edit</a-proj>
                </td>
            </tr>
        }
    </tbody>
</table>
